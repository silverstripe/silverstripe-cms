{
	"name": "cwp/recipe-basic",
	"description": "CWP basic recipe",
	"require": {
		"php": ">=5.3.2",
		"cwp/cwp": "dev-master",
		"cwp/cwp-core": "dev-master",
		"cwp-themes/default": "dev-master",
		"silverstripe/cms": "3.1.1",
		"silverstripe/framework": "3.1.1",
		"silverstripe/advancedworkflow": "dev-master",
		"silverstripe/documentconverter": "dev-master",
		"silverstripe/fulltextsearch": "dev-master",
		"silverstripe/html5": "dev-master",
		"silverstripe/iframe": "dev-master",
		"silverstripe/queuedjobs": "dev-master",
		"silverstripe/multivaluefield": "dev-master",
		"silverstripe/registry": "dev-master",
		"silverstripe/restfulserver": "dev-master",
		"silverstripe/subsites": "dev-master",
		"silverstripe/translatable": "dev-master",
		"silverstripe/taxonomy": "dev-master",
		"silverstripe/userforms": "dev-master",
		"silverstripe/versionedfiles": "dev-master",
		"silverstripe/versionfeed": "1.0.1",
		"undefinedoffset/sortablegridfield": "dev-master"
	},
	"require-dev": {
		"phpunit/phpunit": "3.7.*@stable"
	},
	"config": {
		"process-timeout": 900
	},
	"repositories": [
		{
			"type": "composer",
			"url": "https://packages.cwp.govt.nz/"
		}
	],
	"minimum-stability": "dev"
}
